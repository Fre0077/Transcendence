generator user {
  provider = "prisma-client-js"
  output = "./generate/user"
}

datasource dbuser {
  provider = "sqlite"
  url      = "file:./user.db"
}

model account {
  id        Int @id @default(autoincrement())
  name      String
  surname   String
  username  String @unique
  email     String @unique
  password  String
}