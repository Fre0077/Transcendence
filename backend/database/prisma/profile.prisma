generator profile {
  provider = "prisma-client-js"
  output = "./generate/profile"
}

datasource dbuser {
  provider = "sqlite"
  url      = "file:./profile.db"
}

model dashboard {
  id		Int @id @default(autoincrement())
  userid	number @unique
  user		chats @relation(fields: [userid], references: [userid], onDelete: Cascade)
  score		String
  game		String
}

model user {
  userid		Int @id @default(autoincrement())
  tournament	tournament[]
  username		String @unique
  bios			String
}
